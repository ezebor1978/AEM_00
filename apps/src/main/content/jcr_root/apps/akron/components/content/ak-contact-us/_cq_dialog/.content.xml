<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Contact Us"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <contact-us-form
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Contact Us Form	"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <headingLevel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Title heading level"
                                                name="./headingLevel"
                                                required="{Boolean}false">
                                                <datasource
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="akron/datasources/dropdown"
                                                    path="/apps/akron/dialogs/options/headings"
                                                    defaultValueSelected="h2"
                                                    valueToIgnore="none"/>
                                            </headingLevel>
                                            <first-name-label
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="First name label"
                                                fieldDescription="Default value: First Name. Limit of 30 characters."
                                                required="{Boolean}true"
                                                maxlength="30"
                                                value="First Name"
                                                name="./firstNameLabel"/>
                                            <first-name-placeholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="First name placeholder"
                                                fieldDescription="Default value: Enter first name. Limit of 35 characters."
                                                required="{Boolean}true"
                                                maxlength="35"
                                                value="Enter first name"
                                                name="./firstNamePlaceholder"/>
                                            <last-name-label
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Last name label"
                                                fieldDescription="Default value: Last Name. Limit of 30 characters."
                                                required="{Boolean}true"
                                                maxlength="30"
                                                value="Last Name"
                                                name="./lastNameLabel"/>
                                            <last-name-placeholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Last name placeholder"
                                                fieldDescription="Default value: Enter last name. Limit of 35 characters."
                                                required="{Boolean}true"
                                                maxlength="35"
                                                value="Enter last name"
                                                name="./lastNamePlaceholder"/>
                                            <phone-number-label
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Phone number label"
                                                fieldDescription="Default value: Phone Number. Limit of 30 characters."
                                                required="{Boolean}true"
                                                maxlength="30"
                                                value="Phone Number"
                                                name="./phoneNumberLabel"/>
                                            <phone-number-placeholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Phone number placeholder"
                                                fieldDescription="Default value: Enter last name. Limit of 35 characters."
                                                required="{Boolean}true"
                                                maxlength="35"
                                                value="Enter phone number"
                                                name="./phoneNumberPlaceholder"/>
                                            <email-address-label
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Phone number label"
                                                fieldDescription="Default value: Email Address. Limit of 30 characters."
                                                required="{Boolean}true"
                                                maxlength="30"
                                                value="Email Address"
                                                name="./emailAddressLabel"/>
                                            <email-address-placeholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Email address placeholder"
                                                fieldDescription="Default value: Email Address. Limit of 35 characters."
                                                required="{Boolean}true"
                                                maxlength="35"
                                                value="Email address"
                                                name="./emailAddressPlaceholder"/>
                                            <company-label
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Company label"
                                                fieldDescription="Default value: Company (optional). Limit of 30 characters."
                                                required="{Boolean}true"
                                                maxlength="30"
                                                value="Company (optional)"
                                                name="./companyLabel"/>
                                            <company-placeholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Company placeholder"
                                                fieldDescription="Default value: Enter company name (optional). Limit of 35 characters."
                                                required="{Boolean}true"
                                                maxlength="35"
                                                value="Enter company name (optional)"
                                                name="./companyPlaceholder"/>
                                            <question-label
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="My question is concerning label"
                                                fieldDescription="Default value: My Question is Concerning. Limit of 30 characters."
                                                required="{Boolean}true"
                                                maxlength="30"
                                                value="My Question is Concerning"
                                                name="./questionLabel"/>
                                            <question-placeholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="My question is concerning placeholder"
                                                fieldDescription="Default value: Select an option. Limit of 35 characters."
                                                required="{Boolean}true"
                                                maxlength="35"
                                                value="Select an option"
                                                name="./questionPlaceholder"/>
                                            <question-concerning-captions
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true"
                                                fieldLabel="My question is concerning dropdown options"
                                                required="{Boolean}true"
                                                validation="multifield-max-10">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./multifieldQuestionConcerningOptions">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <column
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <question-concerningOption
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Option"
                                                                    required="{Boolean}true"
                                                                    maxlength="50"
                                                                    name="./option"/>
                                                            </items>
                                                        </column>
                                                    </items>
                                                </field>
                                            </question-concerning-captions>
                                            <customer-type-label
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Customer type label"
                                                fieldDescription="Default value: Customer Type. Limit of 30 characters."
                                                required="{Boolean}true"
                                                maxlength="30"
                                                value="Customer Type"
                                                name="./customerTypeLabel"/>
                                            <customer-type-bussiness
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Customer type: business radio button label"
                                                fieldDescription="Default value: Business. Limit of 30 characters."
                                                required="{Boolean}true"
                                                maxlength="30"
                                                value="Business"
                                                name="./customerTypeBusiness"/>
                                            <customer-type-residential
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Customer type: residential radio button label"
                                                fieldDescription="Default value: Residential. Limit of 30 characters."
                                                required="{Boolean}true"
                                                maxlength="30"
                                                value="Residential"
                                                name="./customerTypeResidential"/>
                                            <questions-or-comments-label
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Questions or comments label"
                                                fieldDescription="Default value: Questions or Comments. Limit of 30 characters."
                                                required="{Boolean}true"
                                                maxlength="30"
                                                value="Questions or Comments"
                                                name="./questionsOrCommentsLabel"/>
                                            <questions-or-comments-placeholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Questions or comments placeholder	"
                                                fieldDescription="Default value: Enter your questions or comments. Limit of 30 characters."
                                                required="{Boolean}true"
                                                maxlength="30"
                                                value="Enter your questions or comments"
                                                name="./questionsOrCommentsPlaceholder"/>
                                            <cta-label
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="CTA label"
                                                fieldDescription="Default value: Submit. Limit of 30 characters."
                                                required="{Boolean}true"
                                                maxlength="30"
                                                value="Submit"
                                                name="./ctaLabel"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </contact-us-form>
                    <explanatory-copy
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Explanatory copy"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <image
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/fileupload"
                                                fieldLabel="Explanatory icon"
                                                fileNameParameter="./fileName"
                                                fileReferenceParameter="./fileReference"
                                                mimeTypes="[image]"
                                                multiple="{Boolean}false"
                                                name="./file"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"
                                                required="{Boolean}true"/>
                                            <explanatory-icon-alternative-text
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Explanatory icon alternative text	"
                                                required="{Boolean}true"
                                                fieldDescription="Limit of 200 characters."
                                                maxlength="200"
                                                name="./explanatoryIconAlternativeText"/>
                                            <explanatory-title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Explanatory title"
                                                required="{Boolean}true"
                                                fieldDescription="Limit of 50 characters."
                                                maxlength="50"
                                                name="./explanatoryTitle"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Description"
                                                fieldDescription="Limit of 200 characters."
                                                required="{Boolean}true"
                                                maxlength="200"
                                                name="./description"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </explanatory-copy>
                    <successScreen
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Success Screen"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <cta-link
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                fieldLabel="CTA Link"
                                                name="./ctaLink"
                                                rootPath="/content/akron"
                                                required="{Boolean}true"/>
                                            <cta-link-target
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                name="./ctaLinkTarget"
                                                fieldLabel="CTA Target"
                                                required="{Boolean}true">
                                                <datasource
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="akron/datasources/dropdown"
                                                    path="/apps/akron/dialogs/options/target-options"
                                                    defaultValueSelected="internal"/>
                                            </cta-link-target>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </successScreen>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
