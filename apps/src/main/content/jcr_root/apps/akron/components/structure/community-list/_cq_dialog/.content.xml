<?xml version="1.0" encoding="UTF-8"?>
<jcr:root
    xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    xmlns:cq="http://www.day.com/jcr/cq/1.0"
    xmlns:jcr="http://www.jcp.org/jcr/1.0"
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Community Programs Page"
    mode="edit">
    <content jcr:primaryType="nt:unstructured">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <biograpy
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Community Programs"
                        sling:orderAfter="advanced"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <fieldset
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Community Programs - Details"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <layout
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <column
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <communitySubtitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        cq:showOnCreate="{Boolean}true"
                                                        fieldLabel="Subtitle"
                                                        maxlength="35"
                                                        fieldDescription="Limit of 35 characters"
                                                        required="{Boolean}true"
                                                        name="./communitySubtitle"/>
                                                    <phone-title
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        cq:showOnCreate="{Boolean}true"
                                                        fieldLabel="Phone Title"
                                                        maxlength="20"
                                                        fieldDescription="Limit of 20 characters"
                                                        required="{Boolean}true"
                                                        name="./phoneTitle"/>
                                                    <phone-number
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        cq:showOnCreate="{Boolean}true"
                                                        fieldLabel="Phone Number"
                                                        fieldDescription="The Phone number has to follow this format: 1-XXX-XXX-XXXX"
                                                        name="./phoneNumber">
                                                        <granite:data
                                                            jcr:primaryType="nt:unstructured"
                                                            cq-dialog-phone-number-validation="true">
                                                        </granite:data>
                                                    </phone-number>
                                                    <phone-icon
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                        cq:showOnCreate="{Boolean}true"
                                                        fieldLabel="Phone Icon"
                                                        name="./phoneIcon"
                                                        rootPath="/content/dam"
                                                        required="{Boolean}true"/>
                                                    <filter-placeholder
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        cq:showOnCreate="{Boolean}true"
                                                        fieldLabel="Filter Placeholder"
                                                        maxlength="20"
                                                        fieldDescription="Limit of 20 characters"
                                                        required="{Boolean}true"
                                                        name="./filterPlaceholder"/>
                                                    <column-1-title
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        cq:showOnCreate="{Boolean}true"
                                                        fieldLabel="Column #1 title"
                                                        maxlength="15"
                                                        fieldDescription="Limit of 15 characters"
                                                        required="{Boolean}true"
                                                        name="./column1Title"/>
                                                    <column-2-title
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        cq:showOnCreate="{Boolean}true"
                                                        fieldLabel="Column #2 title"
                                                        maxlength="15"
                                                        fieldDescription="Limit of 15 characters"
                                                        required="{Boolean}true"
                                                        name="./column2Title"/>
                                                    <column-3-title
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        cq:showOnCreate="{Boolean}true"
                                                        fieldLabel="Column #3 title"
                                                        maxlength="15"
                                                        fieldDescription="Limit of 15 characters"
                                                        required="{Boolean}true"
                                                        name="./column3Title"/>
                                                    <column-4-title
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        cq:showOnCreate="{Boolean}true"
                                                        fieldLabel="Column #4 title"
                                                        maxlength="15"
                                                        fieldDescription="Limit of 15 characters"
                                                        required="{Boolean}true"
                                                        name="./column4Title"/>
                                                </items>
                                            </column>
                                        </items>
                                    </fieldset>
                                </items>
                            </column>
                        </items>
                    </biograpy>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
