<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Template"
    jcr:title="Akron - Community Page"
    cq:designPath="/apps/settings/wcm/designs/akron"
    allowedPaths="[/content/akron/(.*)?]"
    ranking="{Long}100">
    <jcr:content
        is_enrollment="{Boolean}true"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="akron/components/structure/community">
        <ak-header
            jcr:primaryType="nt:unstructured"
            sling:resourceType="akron/components/full/ak-header"/>
        <ak-par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/parsys">
            <ak-community-frame
                jcr:primaryType="nt:unstructured"
                sling:resourceType="akron/components/full/ak-community-frame"/>
            <ak-tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="akron/components/full/ak-tabs-navigation">
                <multifieldItems
                    jcr:primaryType="nt:unstructured">
                    <item0
                        jcr:primaryType="nt:unstructured"
                        title="Residential"/>
                    <item1
                        jcr:primaryType="nt:unstructured"
                        title="Small Business"/>
                </multifieldItems>
                <ak-tab-par1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/parsys">
                    <ak-container
                        jcr:primaryType="nt:unstructured"
                        bgColor="#f8f8fa"
                        bgType="color"
                        sling:resourceType="akron/components/full/ak-container">
                        <ak-container-par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/parsys">
                            <ak-title
                                jcr:primaryType="nt:unstructured"
                                overrideLevel="h4"
                                sling:resourceType="akron/components/content/ak-title"/>
                            <ak-offer-container
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="akron/components/data/ak-offer-container">
                            </ak-offer-container>
                        </ak-container-par>
                    </ak-container>
                </ak-tab-par1>
                <ak-tab-par2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/parsys">
                    <ak-container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="akron/components/full/ak-container">
                        <ak-container-par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/foundation/components/parsys">
                            <ak-offer-container
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="akron/components/data/ak-offer-container"/>
                        </ak-container-par>
                    </ak-container>
                </ak-tab-par2>
            </ak-tabs>
            <ak-container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="akron/components/full/ak-container"/>
        </ak-par>
        <ak-footer
            jcr:primaryType="nt:unstructured"
            sling:resourceType="akron/components/full/ak-footer"/>
    </jcr:content>
</jcr:root>
